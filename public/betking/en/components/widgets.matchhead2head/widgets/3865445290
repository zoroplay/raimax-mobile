(function(SRLive, jQuery, $, undefined) {SRLive.w.define("widgets.matchhead2head",{extend:"common.component",options:{container:"#srlive_matchhead2head",seasonid:4136,matchId:"idContainer_default",emptyRules:{rules:{tendency:{object:"data",requiredData:[{propertyPath:"tendency"}]},match:{object:"data",requiredData:[{propertyPath:"match"}]},teams:{object:"data",requiredData:[{propertyPath:"teams"}]},team_info:{object:"data",requiredData:[{propertyPath:"team_info"}]},lastMatches:{object:"data",requiredData:[{propertyPath:"last_x.home.matches.length"},{propertyPath:"last_x.away.matches.length"}]},nextMatches:{object:"data",requiredData:[{propertyPath:"next_x.home.matches.length"},{propertyPath:"next_x.away.matches.length"}]}},connections:["tendency&&match&&teams&&team_info&&lastMatches","tendency&&match&&teams&&team_info&&nextMatches"]},sportCheck:{object:"data",requiredData:"match._sid"}},data:{},endedMatchStatus:[SREnum.matchStatus.result,SREnum.matchStatus.recentlyended,SREnum.matchStatus.post],constructor:function(){this.callSuper("constructor");this.onGlobal("match_info","on_match_info");this.onGlobal("stats_match_form","on_stats_match_form");this.onGlobal("stats_team_lastx","on_stats_team_lastx");this.onGlobal("stats_team_nextx","on_stats_team_nextx");this.onGlobal("stats_team_versusrecent","on_stats_team_versusrecent");this.requestFeed("match_info",this.options.matchId);this.requestFeed("stats_match_tableslice",this.options.matchId);this.requestFeed("stats_match_form",this.options.matchId);this.requestFeed("stats_team_lastx",this.options.matchId);this.requestFeed("stats_team_nextx",this.options.matchId);this.requestFeed("stats_team_versusrecent",this.options.matchId)},renderData:function(){this.view.render({container:this.getContainer(),template:"main",doc:this.data})},allQueriesReceived:function(){SRUtil.each(this.data,function(t){this.createAliasForHomeAway(t,this.data.match.teams)},this);this.checkVersusRecentData();this.calculateTeamForm();this.render();this.callSuper("allQueriesReceived")},onMatchContainerFocusChange:function(){this.data={};this.getContainer().html("")},createAliasForHomeAway:function(t,a){if(!t||!a||!a.home||!a.home.uid||!a.away||!a.away.uid){return}var e=a.home;var s=a.away;if(t[e.uid]){if(t.home!==undefined){t.home=t[e.uid]}else{Object.defineProperty(t,"home",{enumarable:false,value:t[e.uid]})}}if(t[s.uid]){if(t.away!==undefined){t.away=t[s.uid]}else{Object.defineProperty(t,"away",{enumarable:false,value:t[s.uid]})}}},checkVersusRecentData:function(){if(!this.data.versusrecent||!this.data.match){return}var t=this.data.versusrecent.teams;var a=false;var e=false;SRUtil.each(t,function(t){if(this.data.match.teams.home.uid===t._id){a=true}else if(this.data.match.teams.away.uid===t._id){e=true}},this);if(!(a&&e)){this.data.versusrecent=false;return}var s=this.filterMatches(this.data.versusrecent);this.data.versusrecent=s;if(SRUtil.hasProperty(this.data,"match")){this.createAliasForHomeAway(this.data.versusrecent.meetings,this.data.match.teams)}},calculateTeamForm:function(){if(!this.data.teams||!this.data.last_x){return}this.data.tendency={};SRUtil.each(this.data.teams,function(t){this.data.tendency[t.uid]=this.calculateForm(t.uid)},this);this.createAliasForHomeAway(this.data.tendency,this.data.match.teams)},on_match_info:function(t){if(!t||!t.match||this.getCurrentMatchId()!=t.match._id||!t.match.teams){return}this.data.match=t.match;if(!t.match.teams.home.uid||!t.match.teams.away.uid){return}this.data.teams={};this.data.teams[t.match.teams.home.uid]=t.match.teams.home;this.data.teams[t.match.teams.away.uid]=t.match.teams.away;this.data.matchInfo=t},on_stats_match_form:function(t){if(!t||this.getCurrentMatchId()!=t._id||!t.teams){return}this.data.form={};this.data.form[t.teams.home.team.uid]=t.teams.home.form;this.data.form[t.teams.away.team.uid]=t.teams.away.form},on_stats_team_lastx:function(t){if(!t||!t.team||!t.matches){return}if(!SRUtil.isObject(this.data.last_x)){this.data.last_x={}}if(!SRUtil.is