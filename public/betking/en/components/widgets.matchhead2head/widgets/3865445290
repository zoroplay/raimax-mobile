(function(SRLive, jQuery, $, undefined) {SRLive.w.definePlugin("common.plugin.emptycheck",{isEmpty:function(){var t=this.sportNotSupported(),e=this.missingRequiredData();return t||e},sportNotSupported:function(t){var e=this.options.sportCheck,i=t;if(!e&&!i){return false}var r=SRUtil.getProperty(e,r)||"supportedSports",n=SRUtil.getProperty(this.setup,r);if(!i){var a=e.requiredData.split("."),o=this[e.object];a.unshift(o);i=SRUtil.getProperty.apply(SRUtil,a)}if(!i){return false}var s=false;if(SRUtil.isArray(i)){SRUtil.each(i,function(t){if(SRUtil.inArray(n,t)==-1){s=true}})}else{s=SRUtil.inArray(n,i)==-1}return s},missingRequiredData:function(){var t=this.options.emptyRules;if(!t||!t.connections||t.connections.length<1){return false}var e=false;var i=[];SRUtil.each(t.connections,function(r,n){var a=this.splitConditions(r);if(!e){e=a.outerOperator}var o=[];SRUtil.each(a.data,function(e){var i=t.rules[e];if(!i){throw new Error("Selected rule ("+e+") doesn't exist")}if(SRUtil.isUndefined(this[i.object])||SRUtil.isArray(this[i.object])&&!this[i.object].length){o.push(false);return true}o.push(this.validateRule(i))},this);i.push(this.validateConditions(o,a.innerOperator))},this);return!this.validateConditions(i,e)},splitConditions:function(t){var e={innerOperator:false,outerOperator:false,data:false};if(t.indexOf("||")>-1){e.innerOperator="||";e.outerOperator="&&"}else if(t.indexOf("&&")>-1){e.innerOperator="&&";e.outerOperator="||"}e.data=t.split(e.innerOperator);return e},validateProperty:function(t,e,i){e.unshift(t);var r=SRUtil.getProperty.apply(SRUtil,e);if(i){return SRUtil.inArray(i,r)>=0}return!!r},validateRule:function(t){var e=this[t.object];if(!(t&&t.object&&!SRUtil.isUndefined(e))){return false}var i=true;if(t.requiredData){SRUtil.each(t.requiredData,function(t,r){if(t.propertyPath){if(!this.validateProperty(e,t.propertyPath.split("."),t.allowedValues)){i=false;return false}}else if(SRUtil.isUndefined(e)||e===false){i=false;return false}},this)}else{i=!!e}return i},validateConditions:function(t,e){var i=t[0];if(t.length===1){return i}SRUtil.each(t,function(t){if(e==="||"){i=i||t}else if(e==="&&"){i=i&&t}else{throw new Error("Unknown logical operator "+e)}});return i}});
})(SRLive, window.SRjQuery || jQuery, window.SRjQuery || jQuery);
