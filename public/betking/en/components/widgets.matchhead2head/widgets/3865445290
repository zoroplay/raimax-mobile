})(SRLive, window.SRjQuery || jQuery, window.SRjQuery || jQuery);
SRLive.widget['common.component'] = {'setup': {"dependencies":["common.base","common.events","common.matchmanager","common.matchclock","common.permissions"]}};
(function(SRLive, jQuery, $, undefined) {SRLive.w.define("common.component",{extend:"common.base",options:{matchId:"idContainer_default"},mmFollowedContainers:{},mmFollowedFeedHandles:{},mmPolledFeeds:{},mm:null,mmFeedsArePaused:false,mmPausedFeeds:{},mmPausedPolledFeeds:{},uiActive:false,availableEvents:{match_container_focus_change:true},constructor:function(){this.callSuper("constructor");this.mm=SRLive.w.get("common.matchmanager");this.mc=SRLive.w.get("common.matchclock");this.me=SRLive.w.get("common.events");if(!this.mc){throw new Error("matchclock not initialized")}if(!this.mm){throw new Error("matchmanager not initialized!")}},destructor:function(){SRUtil.each(this.mmFollowedFeedHandles,function(e,t){this.mm.unfollowFeed(e)},this);SRUtil.each(this.mmPolledFeeds,function(e){this.mm.stopFeedPolling(e.feed,e.matchId,this)},this);SRUtil.each(this.mmFollowedContainers,function(e,t){if(e>0){this.mm.unfollowMatch(e)}},this);delete this.mm;delete this.mc;delete this.me;this.callSuper("destructor")},getMatchTime:function(e){return this.mc.getMatchTime(e)},getMMKey:function(e,t){return"mm_"+t+"_"+e},getCurrentMatchId:function(e){if(!this.mm){return false}return this.mm.getMatchId(e||this.options.matchId)},releaseResources:function(e){var t="mm_"+e;var s=[];var i=[];SRUtil.each(this.mmFollowedFeedHandles,function(i,o){if(SRUtil.startsWith(o,t)){var d=o.substring(t.length+1,o.length);this.releaseFeed(d,e);s.push(d)}},this);SRUtil.each(this.mmPolledFeeds,function(t){if(t.matchId===e){this.releasePolledFeed(t.feed,t.matchId);i.push(t.feed)}},this);this.unfollowMatch(e);var o={feeds:s,polledFeeds:i};return o},tryFollowMatch:function(e){e=e||this.options.matchId;if(this.mmFollowedContainers[e]){return}var t=function(e,t,s){SRUtil.each(this.mmFollowedFeedHandles,function(t,s){if(s.indexOf(e)>=0){this.handleQuery(s)}},this);if(SRUtil.isFunction(this.onMatchContainerFocusChange)){this.onMatchContainerFocusChange.apply(this,arguments)}this.trigger("match_container_focus_change")