SRLive.widget['common.events'] = {'setup': {"dependencies":["common.base"]}};
(function(SRLive, jQuery, $, undefined) {SRLive.w.define("common.events",{extend:"common.base",singleton:true,options:{cleanupInterval:6e4,cleanupTimestampOffset:12e4},eventstore:{},eventsCount:0,ADDED_EVENT:1,UPDATED_EVENT:2,REMOVED_EVENT:3,DUPLICATED_EVENT:4,interval:null,initTimestamp:0,initServerTimestamp:0,constructor:function(){this.callSuper("constructor");this.initTimestamp=(new Date).getTime();this.onGlobal("event_get","on_event_get");this.onGlobal("match_timelinedelta","on_match_timelinedelta");var e=this;this.interval=setInterval(function(){e.cleanup()},e.options.cleanupInterval)},destructor:function(){this.callSuper("destructor");clearInterval(this.interval)},triggerEvents:function(e,t,i){if(SRUtil.some(e,function(){return true})){SRLive.event.trigger("events_by_match",{events:e,dob:t._dob,source:i})}},getMatchHolder:function(e,t,i){var n=e[t]=e[t]||{added:[],updated:[],removed:[],match:i};return n},handleEventList:function(e,t,i,n){var s={};if(t&&t._id){this.getMatchHolder(s,t._id,t)}SRUtil.each(e,function(e){if(e.updated_uts&&e.updated_uts*1e3<this.initServerTimestamp){return true}var a=this.getMode(e,n);if(a.type==this.DUPLICATED_EVENT){return true}var r=this.getMatchHolder(s,e.matchid,t||e.match);if(i){r.timelinedelta=true}switch(a.type){case this.ADDED_EVENT:r.added.push(e);this.appendOrReplace(e,n);break;case this.UPDATED_EVENT:r.updated.push(e);this.appendOrReplace(e,n);break;case this.REMOVED_EVENT:r.removed.push(e);this.appendOrReplace(e,n);break}},this);return s},on_match_timelinedelta:function(e,t){this.updateInitTimestamp(t._dob);var i=this.handleEventList(e.events,e.match,true,t._dob);this.triggerEvents(i,t,"match_timelinedelta")},on_event_get:function(e,t){this.updateInitTimestamp(t._dob);var i=this.handleEventList(e,false,false,t._dob);this.triggerEvents(i,t,"event_get")},updateInitTimestamp:function(e){if(!this.initServerTimestamp){var t=(new Date).getTime();this.initServerTimestamp=e*1e3-(t-this.initTimestamp)}},getEventId:function(e){re